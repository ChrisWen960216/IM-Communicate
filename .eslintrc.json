{
    "extends": "react-app",
    "rules": {
        "indent": [1,2],
        "quotes": [1,"single"],
        "semi": [1,"always"],
        "semi-spacing": 0,
        "no-extra-semi": 1,
        "no-trailing-spaces": 2,
        "no-console": 0,
        "prefer-const": 2,
        "space-before-function-paren":"error",
        "camelcase": 0,
        "object-curly-spacing": ["error", "always"],
        "arrow-spacing":"warn",
        "comma-spacing":"warn",
        "space-before-blocks":"warn",
        "keyword-spacing":"warn",
        "key-spacing":"warn"
    },
    "globals": {
        "io": true,
        "socket": true
    }
}